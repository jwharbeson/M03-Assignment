    "def get_odds():\n",
    "    for number in range(10):\n",
    "        if number % 2 != 0:  # Check if the number is odd\n",
    "            yield number\n",
    "\n",
    "# Example usage:\n",
    "odds = get_odds()\n",
    "for i, odd in enumerate(odds, start=1):\n",
    "    if i == 3:\n",
    "        print(odd)\n",
    "        break"
   ]
